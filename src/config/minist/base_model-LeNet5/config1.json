{
    "WORK_FLOW": [
        "train",
        "save",
        "test"
    ],
    "general": {
        "data_split_num": 2,
        "redundancy_num": 1,
        "batch_size": 64,
        "base_model": {
            "args": {
                "input_dim": [
                    1,
                    28,
                    28
                ],
                "num_classes": 10
            },
            "class": "base_model.LeNet5.LeNet5",
            "path": "base_model/LeNet5/MNIST/2023_11_28/model.pth"
        },
        "encoder": {
            "class": "encoder.mlp_encoder.MLPEncoder"
        },
        "decoder": {
            "class": "decoder.mlp_decoder.MLPDecoder"
        }
    },
    "training": {
        "train_dataset": {
            "class": "dataset.splited_dataset.SplitedTrainDataset",
            "path": "data/MNIST/split/{{ data_split_num }}/split_train_datasets.pt"
        },
        "encoder_optimizer": {
            "args": {
                "lr": 0.001,
                "weight_decay": 1e-05
            },
            "class": "torch.optim.Adam"
        },
        "decoder_optimizer": {
            "args": {
                "lr": 0.001,
                "weight_decay": 1e-05
            },
            "class": "torch.optim.Adam"
        },
        "criterion": {
            "class": "torch.nn.MSELoss"
        },
        "scheduler": {
            "args": {
                "factor": 0.1,
                "patience": 10
            },
            "class": "torch.optim.lr_scheduler.ReduceLROnPlateau"
        },
        "epoch_num": 10,
        "save_interval": 1
    },
    "testing": {
        "test_dataset": {
            "class": "dataset.splited_dataset.SplitedTestDataset",
            "path": "data/MNIST/split/{{ data_split_num }}/split_test_datasets.pt"
        },
        "lose_device_index": [
            0,
            1
        ]
    },
    "saving": {
        "save_dir": "save/mnist/base_model-LeNet5/"
    },
    "loading": {
        "load_path": "save/mnist/base_model-LeNet5/"
    }
}